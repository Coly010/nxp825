{
  "name": "app1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app1/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/apps/app1"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/apps/app1"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/coverage/apps/app1"
      ],
      "options": {
        "coverage.reportsDirectory": "../../coverage/apps/app1"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "app1:build",
        "spa": true
      }
    },
    "storybook": {
      "options": {
        "port": 4400,
        "config-dir": ".storybook"
      },
      "configurations": {
        "ci": {
          "args": ["--quiet"]
        }
      }
    },
    "build-storybook": {
      "outputs": [
        "{projectRoot}/{options.output-dir}",
        "{workspaceRoot}/{projectRoot}/storybook-static",
        "{options.output-dir}",
        "{options.outputDir}",
        "{options.o}"
      ],
      "options": {
        "config-dir": ".storybook",
        "output-dir": "../../dist/storybook/app1"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
